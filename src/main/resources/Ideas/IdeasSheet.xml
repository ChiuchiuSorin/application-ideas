{{html wiki="true"}}
$xwiki.ssx.use("Ideas.IdeasSheet")
#set ($discard = $services.localization.use('document', 'Ideas.IdeasTranslations'))
$xwiki.ssx.use('AppWithinMinutes.ClassSheetGenerator')
#set($discard = $doc.use('Ideas.IdeasClass'))
## We don't have access to the form element to set the CSS class for the vertical form layout standard.
#if($context.action=="view")
$doc.display("nbvotes")
$doc.display("nbagainst")

(% id="display_all" class="estimate_cost"%)
(((
&lt;table&gt;
   &lt;thead&gt;
     &lt;tr&gt;
         &lt;th&gt;$msg.get('ideas.ideassheet.estimated')&lt;/th&gt;
         &lt;th&gt;$msg.get('ideas.ideassheet.stat')&lt;/th&gt;
         &lt;th&gt;$msg.get('ideas.ideassheet.priority') &lt;/th&gt;
         &lt;th&gt;$msg.get('ideas.ideassheet.assignement')&lt;/th&gt;
     &lt;/tr&gt;
   &lt;/thead&gt;
   &lt;tbody&gt;
     &lt;tr&gt;
         &lt;td&gt;$doc.display("nbestimate")&lt;/td&gt;
         &lt;td&gt;$doc.display('status')&lt;/td&gt;
         &lt;td&gt;$doc.display("priority")&lt;/td&gt;
         &lt;td&gt;$doc.display("assignement")&lt;/td&gt;
     &lt;/tr&gt;
   &lt;/tbody&gt;
&lt;/table&gt;
)))
#end
(% class="xform" %)
(((
#if($context.action=="edit")
  ; &lt;label for="Ideas.IdeasClass_0_title1"&gt;$escapetool.xml($doc.displayPrettyName('title1', false, false))&lt;/label&gt;##
    (% class="xHint" %)$msg.get('')
  : $doc.display('title1')
  ; &lt;label for="Ideas.IdeasClass_0_nbestimate"&gt;$escapetool.xml($doc.displayPrettyName('nbestimate', false, false))&lt;/label&gt;##
    (% class="xHint" %)$msg.get('')
  : $doc.display('nbestimate')
  ; &lt;label for="Ideas.IdeasClass_0_status"&gt;$escapetool.xml($doc.displayPrettyName('status', false, false))&lt;/label&gt;##
    (% class="xHint" %)$msg.get('')
  : $doc.display('status')
  ; &lt;label for="Ideas.IdeasClass_0_priority"&gt;$escapetool.xml($doc.displayPrettyName('priority', false, false))&lt;/label&gt;##
    (% class="xHint" %)$msg.get('')
  : $doc.display('priority')
  ; &lt;label for="Ideas.IdeasClass_0_assignement"&gt;$escapetool.xml($doc.displayPrettyName('assignement', false, false))&lt;/label&gt;##
    (% class="xHint" %)$msg.get('')
  : $doc.display('assignement')
  ; &lt;label for="Ideas.IdeasClass_0_content1"&gt;$escapetool.xml($doc.displayPrettyName('content1', false, false))&lt;/label&gt;##
    (% class="xHint" %)$msg.get('')
#end
(% class="content_left"%)
(((
  ----
  : $doc.display('content1')
  ----
  &lt;br&gt;
)))
#if($context.action=="view" &amp;&amp; $xwiki.checkAuth())
(% class="forvote"%)
(((
  #set($userList = $doc.getValue('supporters'))
  #set ($nb_sup = $stringtool.countMatches($userList, ","))
  &lt;div class="vote-image"&gt;
    #if($stringtool.contains("$userList","$xwiki.checkAuth()"))
  [[image:Ideas.IdeasSheet@voted.png]] $msg.get('ideas.ideassheet.voted')
    #end
  &lt;/div&gt;
  #if ($nb_sup &lt; 6)
  ; &lt;label for="Ideas.IdeasClass_0_supporters"&gt;$escapetool.xml($doc.displayPrettyName('supporters', false, false))&lt;/label&gt;##
  (% class="xHint" %)$msg.get('')
  : $doc.display('supporters')
  #else
  &lt;div id="disp_sup" class="hidden"&gt;
  ; &lt;label for="Ideas.IdeasClass_0_supporters"&gt;$escapetool.xml($doc.displayPrettyName('supporters', false, false))&lt;/label&gt;##
  (% class="xHint" %)$msg.get('')
  : $doc.display('supporters')
  &lt;/div&gt;
  &lt;div onclick="toggleClass($('disp_sup'), 'hidden')"&gt;
  (% class="on_hover"%)
  (((
  &lt;br&gt;$msg.get('ideas.ideassheet.hidesupp')
  )))
  &lt;/div&gt;
  #end
)))
(% class="againstvote"%)
(((
  #set($userList = $doc.getValue('against'))
  #set ($nb_aga = $stringtool.countMatches($userList, ","))
  &lt;div class="vote-image"&gt;
    #if($stringtool.contains("$userList","$xwiki.checkAuth()"))
  [[image:Ideas.IdeasSheet@votedagainst.png]] $msg.get('ideas.ideassheet.votedagainst')
    #end
  &lt;/div&gt;
  #if ($nb_aga &lt; 6)
  ; &lt;label for="Ideas.IdeasClass_0_against"&gt;$escapetool.xml($doc.displayPrettyName('against', false, false))&lt;/label&gt;##
  (% class="xHint" %)$msg.get('')
  : $doc.display('against')
  #else
  &lt;div id="disp_aga" class="hidden"&gt;
  ; &lt;label for="Ideas.IdeasClass_0_against"&gt;$escapetool.xml($doc.displayPrettyName('against', false, false))&lt;/label&gt;##
  (% class="xHint" %)$msg.get('')
  : $doc.display('against')
  &lt;/div&gt;
  &lt;div onclick="toggleClass($('disp_aga'), 'hidden')"&gt;
  (% class="on_hover"%)
  (((
  &lt;br&gt;$msg.get('ideas.ideassheet.hideagainst')
  )))
  &lt;/div&gt;
  #end
)))
#end
)))
